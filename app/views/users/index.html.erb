<h1> Użytkownicy </h1>
<%= link_to "Dodaj", new_user_path %>
<ul>
<% @users.each do |user| %>
	<li>
		<%= user.name %>
		<%= user.surname %>
		<%= link_to "Edytuj", edit_user_path(user) %>
		<%= link_to "Usuń", user_path(user), method: :delete %>

	</li>
	<% end %>
</ul>
<%= link_to "Zobacz wszystkie posty", posts_path %>